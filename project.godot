config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[android]

modules="org/godotengine/godot/GodotLocalNotification"

[application]

config/name="Appfundum"
run/main_scene="res://scenes/Menu/menu.tscn"
run/low_processor_mode=true
boot_splash/image="res://assets/graph/icons/new-icon.png"
boot_splash/bg_color=Color( 0.137255, 0.152941, 0.231373, 1 )
config/icon="res://assets/graph/icons/new-icon.png"
config/auto_accept_quit=false

[autoload]

global="*res://static/global.gd"
firebase="*res://static/firebase.gd"
analytics="*res://static/analytics.gd"
aL="*res://static/android_lifecycle.gd"
globalTheme="*res://static/theme.gd"
themeStore="*res://assets/theme/theme.gd"
notif="*res://static/notification.gd"
FirebaseNotification="*res://static/Firebase_notification.gd"
AppfundumAchievement="*res://static/AppfundumAchievement.gd"
IAP="*res://static/IAP.gd"

[display]

window/size/width=1440
window/size/height=2560
window/size/test_width="1280"
window/size/test_height="800"
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/handheld/orientation="portrait"
window/ios/hide_home_indicator=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "NativeLib", "ui_design_tool", "zylann.project_stats" )

[filesystem]

import/open_asset_import/use_fbx=false

[gui]

common/text_edit_undo_stack_max_size=5000
theme/use_hidpi=true

[input]

" "={
"deadzone": 0.5,
"events": [  ]
}
achievement_interface_open_close={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":79,"unicode":0,"echo":false,"script":null)
 ]
}

[locale]

fallback="fr_BE"

[rendering]

quality/driver/fallback_to_gles2=true
quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/2d/use_pixel_snap=true
quality/filters/anisotropic_filter_level=8
vram_compression/import_etc=true
quality/shadows/filter_mode=0
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/depth_prepass/enable=false
quality/filters/use_nearest_mipmap_filter=true
quality/subsurface_scattering/weight_samples=false
environment/default_clear_color=Color( 0.137255, 0.152941, 0.231373, 1 )
quality/filters/msaa=4
quality/depth/hdr=false

[world]

2d/cell_size=50
