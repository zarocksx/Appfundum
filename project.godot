; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[android]

modules="org/godotengine/godot/GodotLocalNotification"

[application]

config/name="Appfundum"
run/main_scene="res://scenes/menu.tscn"
boot_splash/image="res://assets/graph/icons/Logo CEI.png"
boot_splash/bg_color=Color( 0.137255, 0.152941, 0.231373, 1 )
config/icon="res://assets/graph/icons/Logo CEI.png"

[autoload]

global="*res://static/global.gd"
notif="*res://static/notification.gd"
firebase="*res://static/firebase.gd"
analytics="*res://static/analytics.gd"
aL="*res://static/android_lifecycle.gd"
theme="res://assets/theme/theme.gd"

[display]

window/size/width=1440
window/size/height=2560
window/size/test_width="1280"
window/size/test_height="800"
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "zylann.project_stats" )

[gui]

theme/use_hidpi=true

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/2d/use_pixel_snap=true
vram_compression/import_etc=true
quality/reflections/texture_array_reflections=false
environment/default_clear_color=Color( 0.137255, 0.152941, 0.231373, 1 )
quality/subsurface_scattering/quality=0
